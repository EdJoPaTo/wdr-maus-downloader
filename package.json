{
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "resilio-sync": "^0.1.0",
    "telegraf": "^3.24.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.6.0",
    "@types/node": "^12.12.6",
    "@types/request-promise-native": "^1.0.17",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "del-cli": "^3.0.0",
    "eslint-config-xo-typescript": "^0.21.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "xo": "^0.25.3"
  },
  "scripts": {
    "build": "del dist && tsc",
    "prepack": "npm run build",
    "start": "ts-node source/index.ts",
    "test": "tsc && xo"
  },
  "xo": {
    "semi": false,
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "rules": {
      "require-atomic-updates": "warn",
      "prefer-named-capture-group": "off",
      "@typescript-eslint/semi": [
        "error",
        "never"
      ]
    },
    "overrides": [
      {
        "files": [
          "index.*"
        ],
        "rules": {
          "camelcase": [
            "warn",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  }
}
